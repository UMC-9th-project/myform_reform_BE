				"properties": {
					"chatRoomId": {
						"$ref": "#/components/schemas/UUID",
						"example": "123e4567-e89b-12d3-a456-426614174000",
						"format": "uuid"
					},
					"image": {
						"type": "string"
					},
					"title": {
						"type": "string"
					},
					"roomType": {
						"type": "string"
					},
					"messageType": {
						"type": "string",
						"enum": [
							"TEXT",
							"IMAGE",
							"OTHER"
						]
					},
					"type": {
						"type": "string",
						"enum": [
							"INQUIRY",
							"ORDER"
						]
					},
					"lastMessage": {
						"$ref": "#/components/schemas/UUID"
					},
					"lastMessageAt": {
						"type": "string",
						"format": "date-time"
					},
					"unreadCount": {
						"type": "number",
						"format": "double",
						"description": "안 읽은 메시지 수",
						"example": 3,
						"minimum": 0
					}
				},
				"required": [
					"chatRoomId",
					"image",
					"title",
					"roomType",
					"messageType",
					"type",
					"lastMessage",
					"lastMessageAt",
					"unreadCount"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Pagenation": {
				"properties": {
					"hasMore": {
						"type": "boolean"
					},
					"nextCursor": {
						"allOf": [
							{
