		"/api/v1/chat/rooms/list/{myId}": {
			"get": {
				"operationId": "GetChatRooms",
				"responses": {
					"200": {
						"description": "채팅방 목록 배열",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/TsoaResponse_ChatRoomListDTO_"
								}
							}
						}
					}
				},
				"summary": "채팅방 목록 조회",
				"tags": [
					"채팅 기능"
				],
				"security": [],
				"parameters": [
					{
						"description": "채팅방 목록을 조회할 사용자 고유 아이디(테스트용)",
						"in": "path",
						"name": "myId",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "채팅방 목록 필터 타입\n- 없음 : 전체 조회\n- INQUIRY: 문의 채팅방\n- ORDER: 주문제작 채팅방\n- UNREAD: 안 읽은 메시지가 있는 채팅방",
						"in": "query",
						"name": "type",
						"required": false,
						"schema": {
							"$ref": "#/components/schemas/ChatRoomFilter"
						}
					},
					{
						"description": "커서 기반 페이지네이션을 위한 커서 값, 마지막으로 조회된 채팅방의 ID",
						"in": "query",
						"name": "cursor",
						"required": false,
						"schema": {
							"type": "string"
						}
					},
					{
						"description": "사용자 유형(테스트용)",
						"in": "query",
						"name": "myType",
						"required": false,
						"schema": {
							"type": "string",
							"enum": [
								"owner",
								"requester"
							]
						}
					}
				]
			}
		},
		"/auth/sms/send": {
			"post": {
				"operationId": "SendSms",
				"responses": {
					"200": {
						"description": "SMS 전송 완료",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/TsoaResponse_SendSmsResponse_"
								},
								"examples": {
									"Example 1": {
										"value": {
											"resultType": "SUCCESS",
											"error": null,
											"success": {
												"statusCode": 200,
												"message": "SMS 전송이 완료되었습니다."
											}
										}
									}
								}
							}
						}
					},
					"400": {
						"description": "전화번호 형식 오류",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ErrorResponse"
								}
							}
						}
					},
					"429": {
						"description": "인증 시도 횟수 초과",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ErrorResponse"
								}
							}
						}
					},
					"500": {
						"description": "서버 내부 오류",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ErrorResponse"
								}
							}
						}
					}
				},
				"description": "입력한 휴대폰 번호로 인증 코드를 발송합니다.",
				"summary": "입력한 휴대폰 번호로 인증 코드를 발송합니다.",
				"tags": [
					"Auth"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SendSmsRequest"
							}
						}
					}
				}
			}
		},
