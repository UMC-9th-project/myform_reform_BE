					"myId": {
						"$ref": "#/components/schemas/UUID",
						"description": "채팅방을 생성하는 사용자 ID (테스트용 - 로그인 구현 전까지 사용)"
					}
				},
				"required": [
					"id",
					"type",
					"myId"
				],
				"type": "object",
				"additionalProperties": false
			},
