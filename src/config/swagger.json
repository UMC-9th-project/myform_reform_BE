		"/api/v1/chat/rooms": {
			"post": {
				"operationId": "CreateChatRoom",
				"responses": {
					"201": {
						"description": "Created",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/TsoaResponse_SimplePostResponseDTO_"
